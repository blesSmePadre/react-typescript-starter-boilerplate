    "eslint-plugin-react-hooks": "^4.2.0",
    "express": "^4.17.1",
    "helmet": "^3.12.0",
    "history": "^4.9.0",
    "hpp": "^0.2.2",
    "html-minifier": "^4.0.0",
    "lodash": "^4.17.15",
    "memory-cache": "^0.2.0",
    "morgan": "^1.9.0",
    "normalizr": "^3.6.0",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.1.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "reselect": "^4.0.0",
